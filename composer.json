{
    "name": "dantleech/phpcr-migrations-bundle",
    "description": "PHPCR migrations bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "phpcr/phpcr-migrations": "^1.1",
        "phpcr/phpcr-implementation": "^2.1",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/console": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.8 || ^2.0",
        "doctrine/phpcr-odm": "^1.4",
        "jackalope/jackalope": "^1.0",
        "jackalope/jackalope-doctrine-dbal": "^1.0",
        "jackalope/jackalope-jackrabbit": "^1.0",
        "symfony/symfony": "^4.4 || ^5.4 || ^6.0",
        "symfony/monolog-bundle": "^2.0 || ^3.0",
        "symfony/phpunit-bridge": "^5.4 || ^6.0",
        "symfony-cmf/testing": "dev-master as 4.5.0"
    },
    "conflict": {
        "symfony/symfony": "3.4.15"
    },
    "autoload": {
        "psr-4": {
            "DTL\\Bundle\\PhpcrMigrations\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
