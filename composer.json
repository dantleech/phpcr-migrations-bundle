{
    "name": "dantleech/phpcr-migrations-bundle",
    "description": "PHPCR migrations bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "phpcr/phpcr-migrations": "^1.1",
        "phpcr/phpcr-implementation": "^2.1",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "symfony/config": "^3.4 || ^4.0 || ^5.0 || ^6.0",
        "symfony/console": "^3.4 || ^4.0 || ^5.0 || ^6.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0 || ^6.0",
        "symfony/http-kernel": "^3.4 || ^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.8 || ^2.0",
        "doctrine/phpcr-odm": "^1.4",
        "symfony/symfony": "^2.8 || ^3.4 || ^4.0 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^2.0 || ^3.0",
        "symfony/phpunit-bridge": "^5.0.5",
        "symfony-cmf/testing": "^2.1 || ^3.0 || ^4.0"
    },
    "conflict": {
        "symfony/symfony": "3.4.15"
    },
    "autoload": {
        "psr-4": {
            "DTL\\Bundle\\PhpcrMigrations\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
